package chap05;

public class Array2DExam01 {

	public static void main(String[] args) {
		int[][] myArr = new int[5][5];
		for(int outer=0;myArr.length;outer++) {
			for(int i=0;myArr[outer].length;i++) {
				myArr[outer][i] = 5*outer + i;
			}
		}
		
		for(int outer=0;myArr.length;outer++) {
			for(int i=0;myArr[outer].length;i++) {
				System.out.println(myArr[outer][i]);
			}
		}
	}
}
