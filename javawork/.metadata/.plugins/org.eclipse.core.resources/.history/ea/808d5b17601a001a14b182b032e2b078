package chap05;

public class Pg14Exam07 {

	public static void main(String[] args) {
		int num = Integer.parseInt(args[0]);
		int sum = 0;
		System.out.println("ÀÔ·Â ¿¹1:"+num);
		for(int i=0;i<=100;i++) {
			if(i%num==0) {
				System.out.print(i);
				if(i!=100) {
					System.out.print("+");
				}
				sum += i;
			}
		}
		System.out.println(sum);
	}

}
