package chap07.oop;
//static변수를 연습할 수 있는 예제
public class StaticDemo {
	String name;
	int num;
	static int staticNum;
	public StaticDemo() {
	}
	public StaticDemo(String name) {
		this.name = name;
		num++;
		staticNum++;
	}
	public void display() {
		System.out.println("name:"+name+",num:"+num+",staticnum:"+staticNum);
	}
}
