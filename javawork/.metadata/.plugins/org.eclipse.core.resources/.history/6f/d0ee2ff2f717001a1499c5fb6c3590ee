package chap04;
//break문 연습 - break문이 선언된 블럭을 빠져나간다.
public class GuGu_Break {
	public static void main(String[] args) {
		for(int dan=2;dan<=9;dan++) {
			if(dan==5) {
				break;
			}
			for(int j=1;j<=9;j++) {
				if(dan==5) {
					break;
				}
				System.out.print(dan+" * "+j+" = "+dan*j+"\t");
			}
			System.out.println();
		}
	}

}
